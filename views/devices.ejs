<%- include('partials/Header',{'username':locals.username,'activeDevices':1}) %>
    <div>
        <h3 class="text-black-50 mb-1">Owned Devices</h3>
    </div>
    
    <div class="row gy-3 justify-content-evenly align-items-center">
        <% for(var device of locals.devices){ %>
        <div class="col-12 offset-xxl-0">
            <div class="vstack">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Arduino ID <%= device.AUTH_ID %></h4>
                        <h6 class="text-muted card-subtitle mb-2">Subtitle...</h6><a href="/devices/<%=device.AUTH_ID%>" class="btn btn-outline-primary btn-sm border rounded-0" role="button">View Graph</a>
                    </div>
                </div>
            </div>
        </div>
        <% }; %>
    </div>
    
<%- include('partials/Footer') %>